        <affected family="macos">
          <platform>Apple Mac OS X</platform>
          <platform>Apple Mac OS X Server</platform>
          <product>Microsoft Office 2008 for Mac</product>
          <product>Microsoft Office 2011 for Mac</product>
        </affected>
        <reference source="CVE" ref_id="CVE-2013-0095" ref_url="http://cve.mitre.org/cgi-bin/cvename.cgi?name=CVE-2013-0095"/>
