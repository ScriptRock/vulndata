		    </oval_repository>
		  </metadata>
		  <criteria>
		    <extend_definition comment="Debian 8 is installed" definition_ref="oval:org.mitre.oval:def:28919" />
		    <criterion comment="zeromq3 is earlier than 0:4.0.5+dfsg-2+deb8u1" test_ref="oval:org.cisecurity:tst:278" />
		  </criteria>
		</definition>
