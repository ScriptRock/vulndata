          </dates>
          <status>INTERIM</status>
          <min_schema_version>5.10</min_schema_version>
        </oval_repository>
      </metadata>
      <criteria operator="AND">
        <criterion comment="HP-UX B.11.31" test_ref="oval:org.mitre.oval:tst:8260" />
        <criteria comment="filesets test" operator="OR">
          <criterion comment="NameService.BIND-AUX version is less than C.9.9.4.8.0" test_ref="oval:org.cisecurity:tst:2400" />
          <criterion comment="NameService.BIND-RUN version is less than C.9.9.4.8.0" test_ref="oval:org.cisecurity:tst:2401" />
        </criteria>
