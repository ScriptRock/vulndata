    <definition id="oval:org.cisecurity:def:1693" class="vulnerability" version="2">
      <metadata>
        <title>Vulnerability in Tivoli LWI impacts pConsole and WebSM for AIX</title>
        <affected family="unix">
          <platform>IBM AIX 6.1</platform>
          <platform>IBM AIX 7.1</platform>
        </affected>
        <reference ref_id="CVE-2016-6038" ref_url="http://cve.mitre.org/cgi-bin/cvename.cgi?name=CVE-2016-6038" source="CVE" />
