  <entry id="CVE-2017-6074">
    <vuln:cve-id>CVE-2017-6074</vuln:cve-id>
    <vuln:published-datetime>2017-02-18T16:59:00.237-05:00</vuln:published-datetime>
    <vuln:last-modified-datetime>2017-02-18T16:59:00.237-05:00</vuln:last-modified-datetime>
